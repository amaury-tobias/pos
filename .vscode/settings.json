{
  "typescript.tsdk": "node_modules/typescript/lib",
  "eslint.validate": ["vue", "javascript", "javascriptreact", "typescript", "typescriptreact"],
  "files.associations": {
    "*.css": "postcss"
  },
  "volar.tsPlugin": true,
  "css.validate": false,
  "windicss.colorDecoratorsType": "bg",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "node_modules": true
  }
}
